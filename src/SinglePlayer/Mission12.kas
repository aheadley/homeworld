FSM GravWellSupport
    States Choose, ServiceGravWell, ServiceCarrier, ServiceCruiser, GiveToAI;

    INIT
            Jump Choose;
    ENDINIT
    WATCH
    ENDWATCH

        STATE Choose //дддддддддддддддддддддддддддддддддддддддддд
            INIT
                    IF (ThisTeamIs (TEAM_GW1Support))
                            IF (ShipsCount (TEAMSHIPS_GravWellTeam1) > 0)
                                    Jump ServiceGravWell;
                            ELSEIF (ShipsCount (TEAMSHIPS_Carrier) > 0)
                                    Jump ServiceCarrier;
                            ELSEIF (ShipsCount (TEAMSHIPS_Cruiser) > 0)
                                    Jump ServiceCruiser;
                            ELSE
                                    Jump GiveToAI;
                            ENDIF
                    ELSEIF (ThisTeamIs (TEAM_GW2Support))
                            IF (ShipsCount (TEAMSHIPS_GravWellTeam2) > 0)
                                    Jump ServiceGravWell;
                            ELSEIF (ShipsCount (TEAMSHIPS_Carrier) > 0)
                                    Jump ServiceCarrier;
                            ELSEIF (ShipsCount (TEAMSHIPS_Cruiser) > 0)
                                    Jump ServiceCruiser;
                            ELSE
                                    Jump GiveToAI;
                            ENDIF
                    ELSEIF (ThisTeamIs (TEAM_GW3Support))
                            IF (ShipsCount (TEAMSHIPS_GravWellTeam3) > 0)
                                    Jump ServiceGravWell;
                            ELSEIF (ShipsCount (TEAMSHIPS_Carrier) > 0)
                                    Jump ServiceCarrier;
                            ELSEIF (ShipsCount (TEAMSHIPS_Cruiser) > 0)
                                    Jump ServiceCruiser;
                            ELSE
                                    Jump GiveToAI;
                            ENDIF
                    ENDIF
            ENDINIT
            WATCH
            ENDWATCH
        ENDSTATE Choose //ддддддддддддддддддддддддддддддддддддддд


        STATE ServiceGravWell //ддддддддддддддддддддддддддддддддд
            INIT
                    IF (ThisTeamIs (TEAM_GW1Support))
                            IF (ShipsCount (TEAMSHIPS_GravWellTeam1) > 0)
                                    AttackSpecial (TEAMSHIPS_GravWellTeam1);
                            ELSE
                                    Jump Choose;
                            ENDIF
