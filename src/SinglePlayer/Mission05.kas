        STATE Guard //ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ
            INIT
                    FormationSphere ();
                    IF (ThisTeamIs (TEAM_Defenders1))
                            IF (ShipsCount (TEAMSHIPS_Frigates1) > 0)
                                    Guard (TEAMSHIPS_Frigates1);
                                    VarCreateSet ("GuardingFrigate1", TRUE);
