                                    VarDestroy ("GuardingCarrier");
                                    Jump Guard;
                            ENDIF
                    ELSEIF (VarGet ("GuardingSupportFrigate") = TRUE)
                            IF (ShipsCount (TEAMSHIPS_SupportFrigate) = 0)
                                    VarDestroy ("GuardingSupportFrigate");
                                    Jump Guard;
