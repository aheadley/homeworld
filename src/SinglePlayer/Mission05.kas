            INIT
            ENDINIT
            WATCH
                    TeamHealthSet (60);                                                                 // Keep resetting health until the player finds them
                    IF (ObjectiveGet ("SendProbe") = TRUE)
                            Jump GuardCarrier;
                    ENDIF
            ENDWATCH
        ENDSTATE Wait //ÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ


