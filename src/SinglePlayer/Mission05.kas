                            IF (ShipsCount (TEAMSHIPS_Frigates3) > 0)
                                    Guard (TEAMSHIPS_Frigates3);
                                    VarCreateSet ("GuardingFrigate3", TRUE);
                            ELSEIF (ShipsCount (TEAMSHIPS_Frigates1) > 0)
                                    Guard (TEAMSHIPS_Frigates1);
