                            ENDIF
                    ELSEIF (ThisTeamIs (TEAM_Destroyers2))
                            IF ( (ShipsCount (SHIPS_Des2Targets) = 0) OR (ShipsOrder (TEAMSHIPS_Destroyers2) != 2) )
                                    Jump ACT3DefendYourself;
