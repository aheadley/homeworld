          INIT
            VarDestroy("WhichPath");
            VarCreateSet("WhichPath", (Random (1, 6)));
          ENDINIT
          WATCH
            IF (VarGet("WhichPath")=1)
              VarCreateSet("G_MyPath", 1);
              PatrolPath(PATH_TuranicCarrier1);
